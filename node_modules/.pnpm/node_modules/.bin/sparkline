#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dana/Documents/code/playlistConverter/node_modules/.pnpm/sparkline@0.1.2/node_modules/sparkline/bin/node_modules:/home/dana/Documents/code/playlistConverter/node_modules/.pnpm/sparkline@0.1.2/node_modules/sparkline/node_modules:/home/dana/Documents/code/playlistConverter/node_modules/.pnpm/sparkline@0.1.2/node_modules:/home/dana/Documents/code/playlistConverter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dana/Documents/code/playlistConverter/node_modules/.pnpm/sparkline@0.1.2/node_modules/sparkline/bin/node_modules:/home/dana/Documents/code/playlistConverter/node_modules/.pnpm/sparkline@0.1.2/node_modules/sparkline/node_modules:/home/dana/Documents/code/playlistConverter/node_modules/.pnpm/sparkline@0.1.2/node_modules:/home/dana/Documents/code/playlistConverter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sparkline/bin/sparkline" "$@"
else
  exec node  "$basedir/../sparkline/bin/sparkline" "$@"
fi
